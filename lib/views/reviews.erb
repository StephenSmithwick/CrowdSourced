<table>
  <tr>
    <th>Is A Review</th>
    <th>Is it A good Review</th>
    <th>time of review</th>
    <th>The Term</th>
    <th>The text of review</th>
  </tr>
<% @reviews.each do |review| %>
      <tr>
        <td><%= "#{review["review"]}" %></td>
        <td><%= "#{review["liked"]}" %></td>
        <td><%= "#{review["time_saved"]}" %></td>
      	<td><%= "#{review["term"]}" %></td>
      	<td><%= "#{review["text"]}" %></td>
      </tr>
<% end %>
</table>